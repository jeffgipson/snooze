<div class="container">
<h1>New Signup</h1>

<%#= render 'form', signup: @signup %>

  <%= form_for @signup do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>

    <%= f.label :last_name %>
    <%= f.text_field :last_name %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :address2 %>
    <%= f.text_field :address2 %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <%= f.label :zip %>
    <%= f.text_field :zip %>

    <%= f.label :phone %>
    <%= f.text_field :phone %>

    <%= f.label :selfie %>
    <%= f.file_field :selfie %>

    <%= f.label :photo_id %>
    <%= f.file_field :photo_id %>

    <%= f.submit  %>
  <% end %>


<%= link_to 'Back', signups_path %>
</div>
<%= @body %>
<%= @token %>